import copy
import re
from collections import deque

def contigSum(i, l):
    for first_i in range(len(l)):
        s = l[first_i]
        last_i = first_i + 1
        while last_i < len(l) and l[last_i] == l[last_i - 1] + 1:
            s += l[last_i]
            if s == i:
                return [first_i, last_i]
            if s > i:
                break
            last_i += 1

def gauss(t):
    return t * (t + 1) / 2

def check(i, d, s):
    for k in range(len(d)):
        for l in range(k + 1, len(d)):
            #print("k {} l {}".format(k, l))
            if d[k] + d[l] == i:
                #print(d[k] + " + " + d[l] + " = " + i)
                return True
    return False

d = deque()
s = set()
l = []

with open("input") as f:
    for l in f:
        try:
            i = int(str(l[0:-1]))
            l.append(i)
        except Exception as e:
            print(e)
            while True:
                continue
            continue

l.sort()

input = 556543474 # part 1

g = contigSum(input, l)
print(gauss(g[1]) - gauss(g[0] - 1))
